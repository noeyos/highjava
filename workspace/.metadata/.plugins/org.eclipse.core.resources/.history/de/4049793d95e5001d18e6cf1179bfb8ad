public class ThreadTest15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

// 공통으로 사용할 클래스
class ShareObject {
	private int sum = 0;
	
	public void add() {
		int n = sum;
		n += 10;
		sum = n;
		
		System.out.println(Thread.currentThread().getName() + " 합계 : " + sum);
	}
	
}
